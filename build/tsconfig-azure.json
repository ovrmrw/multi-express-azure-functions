  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": false,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2017"
    ]
  },
  "include": [
    "../**/*.ts"
  ],
  "exclude": [
    "../test",
    "../node_modules",
    "../jspm_packages"
  ]
}